{
  "monkeys": [
    {
      "_id": "_design/monkeys",
      "views": {
        "all": {
          "map": "function (doc) { if (doc.color) emit(doc._id, doc) }"
        },
        "auditDocsByOriginId": {
          "map": "function (doc) { if (doc.type === 'audit' && doc['a_metadata']['originId']) emit(doc['a_metadata']['originId'], doc) }"
        }
      }
    }
  ]
}